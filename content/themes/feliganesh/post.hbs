<!DOCTYPE html>
<html>
{{> head}}

{{#post}}  

   <!--Filtering content-->
  {{#has tag="blog"}}
    {{> blog-post title=this.title content=this.content image=this.image}}
  {{/has}}


  {{#has tag="portfolio"}}
    {{> portfolio-post title=this.title content=this.content image=this.image}}
  {{/has}}

{{/post}}


</html>
