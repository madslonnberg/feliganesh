<script>
	$(document).ready(function() {

		activateBoxesLinks();	


		$(this).on('endHeadAnimation', function() {
			$('svg.menu').css('display', 'block');
			$('svg.head').css('display', 'none');

			$('#triangle, #boxes, #texts, #light-paths, #lines').css('opacity',0);
		
			var duration = 10;
			animateTriangleStroke(duration);
			showLines(30);
			
			setTimeout(function(){
				showBoxes(1,30);
			}, duration*450);
		});
	
	});

	      

	function activateBoxesLinks(){
		$('.menu-item').click(function(e){
			e.stopPropagation();
			var link = $(this).data('href');
			window.location.href = link;
		});
	}

	function showBoxes(duration, delay){
		var selectors = '#boxes, #texts';
		
		anime({
			targets: selectors,
			'opacity': 1,
			delay: function(el, index){
				return index * 3000;
			},
			duration: duration * 1000 
		});
		
		
		
		$('text').mouseover(function(){
			$(this).css('cursor', 'pointer');
			// $(this).prev().css({
			// 'fill':'#657e0c',
			// 'stroke': 'none',
			
			// })

			$(this).prev().addClass('selected');
		}).mouseleave(function(){
			$(this).css('cursor', 'default');
			$(this).prev().removeClass('selected');
		})
	}

	function showLines(duration){
		anime({
			targets: '#lines, #light-paths',
			'opacity': 1,
			duration: duration * 1000 
		});
	}

	function animateTriangleStroke(duration) {
		var selector = '#triangle path';
		var length = getStrokeLength(selector);
		
		$('#triangle').css('opacity', '0.8');

		$(selector).css({
			'stroke-dasharray': length,
			'stroke-dashoffset': length
		});

		anime({
			targets: '#triangle path',    
			duration: duration * 1000,
			delay: duration * 500 - 600,
			'fill-opacity': 1    
		});  
		
		
		anime({
			targets: [selector],
			'stroke-dashoffset': 0,
			duration: duration * 1000,    
			easing: 'easeInOutQuad'
		});
	}

	function animateTriangleFillOpacity(duration) {
		var selector = '#triangle path';

		$(selector).css({
			'fill-opacity': 0,
		})

		anime({
			targets: [selector],
			duration: duration * 1000,
			'fill-opacity': 1,
			easing: 'linear',
		});
	}

	function getStrokeLength(selector) {
		var path = document.querySelector(selector);
		return path.getTotalLength();
	}

	
</script>
